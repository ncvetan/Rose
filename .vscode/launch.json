    "version": "0.2.0",
    "configurations": [
        {
            "name": "x64-debug-launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/x64-Debug/rose/rose.exe",
            "symbolSearchPath": "${workspaceFolder}/out/build/x64-Debug/rose/Debug",
            "externalConsole": true,
            "cwd": "${workspaceFolder}"
          },
          {
            "name": "x64-release-launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/x64-Release/rose/rose.exe",
            "externalConsole": true,
            "cwd": "${workspaceFolder}"
          }
    ]
}